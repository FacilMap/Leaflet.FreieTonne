    "dev-server": "vite",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.10.5",
    "leaflet": "^1.9.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.6.4"
  },
